{
  "version": 2,
  "env": {
    "SQL_USERNAME": "@cso-sql-username",
    "SQL_PASSWORD": "@cso-sql-password",
    "SQL_HOSTNAME": "@cso-sql-hostname",
    "SQL_DATABASE": "@cso-sql-database",
    "ELEVATION_API_KEY": "@cso-elevation-api-key"
  },
  "build": {
    "env": {
      "SQL_USERNAME": "@cso-sql-username",
      "SQL_PASSWORD": "@cso-sql-password",
      "SQL_HOSTNAME": "@cso-sql-hostname",
      "SQL_DATABASE": "@cso-sql-database"
    }
  },
  "builds": [
    { "src": "src/api/index.js", "use": "@now/node" },
    { "src": "src/import/index.js", "use": "@now/node" },
    { "src": "package.json", "use": "@now/static-build"}
  ],
  "routes": [
    { "src": "/", "headers": { "cache-control": "s-maxage=31536000" }, "dest": "/src/api/index.js" },
    { "src": "/import", "headers": { "cache-control": "s-maxage=31536000" }, "dest": "/src/import/index.js" },
    { "src": "/snapshots/geojson", "dest": "/snapshots/geojson.txt"},
    { "src": "/snapshots/csv", "dest": "/snapshots/csv.txt"},
    { "src": "/snapshots/json", "dest": "/snapshots/json.txt"},
    { "src": "/docs", "dest":"/docs.html"}
  ]
}
